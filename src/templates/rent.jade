								a(href="#") Brokers
							li
								a(href="#") Blog
		#mainContainer.container(itemscope,itemtype="https://schema.org/RentAction")
			.row
				.col-md-8.left-side
					section.house-overview.top.shadow-box.clearfix
						h3.sr-only House info
						ul
							li.icon-pin Appartment
							li.icon-pin 3 beds
							li.icon-pin 2 bathes
							li.icon-pin 1.350 sqft
						span.price.icon-bookmark(itemprop="price")
							span.amount 250,000
							span.unit AED/year
					section.house-carousel.shadow-box
						h3.sr-only House images
						#house-carousel.carousel.slide(data-ride="carousel")
							- var slides = [1, 2, 3, 4, 5, 6, 7]
							.carousel-inner(role="listbox")
								each val in slides
									.item(class= val == 1 ? 'active' : '')
										img(src="assets/img/wide.jpg" , alt="The Reserve image #{val}",itemprop="image")
							ol.carousel-indicators
								each val in slides
									li(data-target="#house-carousel" , data-slide-to="#{val - 1}" class= val == 1 ? 'active' : '', style="background-image: url('assets/img/#{val}.jpg')")
										img.sr-only(src="assets/img/#{val}.jpg" , alt="The Reserve image #{val} - Thumbnail")
