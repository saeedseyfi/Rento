				aside.col-md-4.right-side
					section#senderDetails.side-box.shadow-box.clearfix
						h3.sr-only Agent details
						div
							figure.avatar
								img(src="assets/img/fig.jpg", alt="Jennifer Doe")
							.details
								h3.name(itemprop="agent") Jennifer Doe
								p.more-details
									span The Reserve Agency LLC
									span Emaar Square
									span Downtown, Dubai, UAI
								figure.logo
									img(src="assets/img/aff.jpg", alt="Land Sterling")
					section#contact.side-box.shadow-box.clearfix
						h3.sr-only Contact
						#contactAccordion.panel-group(role='tablist', aria-multiselectable='true')
							.panel.panel-default
								#contactByPhone.panel-heading(role='tab')
									h4.panel-title
										a.icon-grid.collapsed(role='button', data-toggle='collapse', data-parent='#contactAccordion', href='#collapseContactByPhone', aria-expanded='true', aria-controls='collapseContactByPhone') Contact by phone
								#collapseContactByPhone.panel-collapse.collapse(role='tabpanel', aria-labelledby='contactByPhone')
									.panel-body 055 123 45 67
							.panel.panel-default
								#contactByEmail.panel-heading(role='tab')
									h4.panel-title
										a.icon-email.collapsed(role='button', data-toggle='collapse', data-parent='#contactAccordion', href='#collapceContactByEmail', aria-expanded='false', aria-controls='collapceContactByEmail') Contact by email
								#collapceContactByEmail.panel-collapse.collapse(role='tabpanel', aria-labelledby='contactByEmail')
									.panel-body.form
										form
											textarea.form-control(rows="3", placeholder="Type your message to the agent")
											input.form-control(type="text", placeholder="Full name")
											input.form-control(type="email", placeholder="Email address")
											input.form-control(type="tel", placeholder="Phone")
											button.btn.btn-primary(type="submit") Send
							.panel.panel-default
								#makeAnOffer.panel-heading(role='tab')
									h4.panel-title
										a.icon-edit-write.collapsed(role='button', data-toggle='collapse', data-parent='#contactAccordion', href='#collapceMakeAnOffer', aria-expanded='false', aria-controls='collapceMakeAnOffer') Make an offer
								#collapceMakeAnOffer.panel-collapse.collapse(role='tabpanel', aria-labelledby='makeAnOffer')
									.panel-body.form
										form
											.form-group
												label.control-label(for='makeAnOfferAmount') Your offer
												.input-group
													input#makeAnOfferAmount.form-control(type='text')
													span.input-group-addon AED/year
												span.note.small only serious and reasonable offers will be considered by the broker
											input.form-control(type="text", placeholder="Full name")
											input.form-control(type="email", placeholder="Email address")
											input.form-control(type="tel", placeholder="Phone")
											button.btn.btn-primary(type="submit") Send
					section#ad.side-box.clearfix.hidden-xs.hidden-sm
						h6.text-uppercase Advertisements
